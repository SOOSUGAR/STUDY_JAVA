
class Student {
	private String name;
	private int kor;
	private int eng;
	private int mat;
	private int sum;
	private double ave;
	
	public Student(String name, int kor, int eng, int mat, int sum, double ave) {
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
		this.sum = sum;
		this.ave = ave;
	}
	
	public String getName() {
		return name;
	}
	
	public int getKor() {
		return kor;
	}
	
	public int getEng() {
		return eng;
	}
	
	public int getMat() {
		return mat;
	}
	
	public int getSum() {
		return (kor + eng + mat);
	}
	
	public double getAve() {
		return ((double)sum / 3);
	}
		
	public void setName(String name) {
		this.name = name;
	}
	
	public void setKor(int kor) {
		this.kor = kor;
	}
	
	public void setEng(int eng) {
		this.eng = eng;
	}
	
	public void setMat(int mat) {
		this.mat = mat;
	}
	
	public void setSum(int sum) {
		this.sum = sum;
	}
	
	public void setAve(double ave) {
		this.ave = ave;
	}
}


public class ProgrammingTest04 {
	public static void main(String[] args) {
		
		
		
	}

}

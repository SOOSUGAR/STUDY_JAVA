import java.util.ArrayList;

public class ProgrammingTest02 {
	public static void main(String[] args) {
		String data = "95,87,77,65,93,58,72,84,93,75,73,88";
		
		System.out.println("- 1¹ø -");
		String[] scores = data.split(",");
		for(int i = 0; i < scores.length; i++) {
			System.out.print(scores[i] + " ");
		}
		
		System.out.println("- 2¹ø -");
		ArrayList<Integer> list = new ArrayList<Integer>();
		int sum = 0;
		double ave = 0.0;
		for(int i = 0; i<scores.length; i++) {
			list.add(Integer.parseInt(scores[i]));
		}
		System.out.println("list : " + list);
		
		
		System.out.println("- 3¹ø -");
		System.out.println("ÃÑÁ¡ : " + sum);
		
		System.out.println("- 4¹ø -");
		System.out.println("Æò±Õ : " + ave);
	}

}
